      {% block header %}
      {% include 'header.njk' %}
      {% endblock %}
    </div>

    <div class="content row">
      <div class="medium-8 medium-push-4 column content-menu">
        {% block content %}
        {% endblock %}
      </div>

      <div class="medium-4 medium-pull-8 column content-side">
        {% block sidebar %}
        {% include 'sidebar.njk' %}
        {% endblock %}
      </div>

      <img class="small-12 column content-footer" src="assets/images/content-footer.png" />
    </div>
  </div>
